@{
    Layout = "~/Views/header.cshtml";
}

<h1>
    Home Index
    @ViewData["hello"]
</h1>

@{
    var std = (List<mvc_dot_net_dive_in_hands_on_dirty.Models.Students>)ViewData["std"];
    foreach (var s in std)
    {
    <h2>ID: @s.id</h2>
    <h2>Name: @s.name</h2>
    <h2>Email: @s.email</h2>
    <h2>Age: @s.age</h2>
    }

}